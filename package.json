{
  "name": "lightcord",
  "version": "0.1.9",
  "description": "A simple - customizable - Discord Client.",
  "main": "dist/index.js",
  "scripts": {
    "compile": "node scripts/compile.js",
    "test": "yarn compile && electron .",
    "run": "electron .",
    "build": "yarn build:minify && yarn build:electron",
    "build:electron": "node scripts/build_electron.js",
    "build:electron_win": "electron-builder build --win",
    "build:electron_linux": "electron-builder build --linux",
    "build:electron_mac": "electron-builder build --mac",
    "start": "electron .",
    "build:minify": "node scripts/build.js",
    "clean": "node scripts/clean.js",
    "clean:all": "node scripts/clean.js --all",
    "devInstall": "node scripts/devInstall.js",
    "versions": "echo Electron: && electron -v && echo Typescript: && tsc -v",
    "ci": "node scripts/ci-deploy",
    "lint": "eslint src/**.ts --fix && prettier -w src/**.ts"
  },
  "author": "JeanOUINA",
  "license": "MIT",
  "dependencies": {
    "auto-launch": "^5.0.5",
    "glasstron": "0.1.1",
    "mkdirp": "^1.0.4",
    "node-fetch": "^3.0.0",
    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "tmp": "^0.2.1",
    "yauzl": "^2.10.0"
  },
  "private": true,
  "devDependencies": {
    "@types/auto-launch": "^5.0.2",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/micromatch": "^4.0.2",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "16.11.6",
    "@types/rimraf": "^3.0.2",
    "@types/uuid": "^8.3.1",
    "@types/yauzl": "^2.9.2",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "asar": "^3.1.0",
    "dotenv": "^10.0.0",
    "electron": "^15.3.0",
    "electron-builder": "^22.13.1",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "fast-glob": "^3.2.7",
    "fs-extra": "^10.0.0",
    "micromatch": "^4.0.4",
    "prettier": "^2.4.1",
    "terser": "^5.9.0",
    "typescript": "^4.4.4",
    "yazl": "^2.5.1"
  }
}
